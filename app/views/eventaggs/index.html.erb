<%- model_class = Eventagg -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:link) %></th>
      <th><%= model_class.human_attribute_name(:site_type) %></th>
      <th><%= model_class.human_attribute_name(:pagination) %></th>
      <th><%= model_class.human_attribute_name(:domain) %></th>
      <th><%= model_class.human_attribute_name(:eventpage) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @eventaggs.each do |eventagg| %>
      <tr>
        <td><%= link_to eventagg.id, eventagg_path(eventagg) %></td>
        <td><%= eventagg.name %></td>
        <td><%= eventagg.link %></td>
        <td><%= eventagg.site_type %></td>
        <td><%= eventagg.pagination %></td>
        <td><%= eventagg.domain %></td>
        <td><%= eventagg.eventpage %></td>
        <td><%=l eventagg.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_eventagg_path(eventagg), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      eventagg_path(eventagg),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_eventagg_path,
            :class => 'btn btn-primary' %>
